<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nyc.c4q.ramonaharrison.startupquiz" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstQuestion"
            android:label="@string/title_activity_first_question" >
        </activity>
        <activity
            android:name=".SecondQuestion"
            android:label="@string/title_activity_second_question" >
        </activity>
        <activity
            android:name=".ThirdQuestion"
            android:label="@string/title_activity_third_question" >
        </activity>
        <activity
            android:name=".Answer"
            android:label="@string/title_activity_answer" >
        </activity>
    </application>

</manifest>
